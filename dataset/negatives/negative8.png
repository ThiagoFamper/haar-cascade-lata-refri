r-radius: 0;\n  border: 8px solid transparent;\n  border-image: url(https://popmenucloud.com/izyapxve/5f7b0157-01b3-4061-80fb-6dfdc452ac7b.png) 30 / 8px / 8px round;\n\n  .pm-acf-header, .pm-acf-subheader {\n    text-transform: uppercase;\n  }\n\n  .pm-acf-divider {\n    margin-top: 15px;\n    color: $white;\n  }\n\n  /* checkboxes */\n  .pm-h3, p.MuiTypography-root.MuiTypography-body1, .pm-acf-description, .pm-acf-header, .pm-acf-checkbox div > div, .pm-acf-subheader { \n    color: $white;\n  }\n  .pm-acf-checkbox div > div,  .pm-acf-multiple-choice {\n    color: $white;\n\n    .MuiSvgIcon-root {\n      color: $tertiary-color;\n    }\n  }\n\n  .MuiInputBase-root :is(p,section:where(.pm-about-section-content)) {\n    color: $white !important;\n  }\n\n  .pm-form-group p.MuiTypography-root.MuiTypography-body1 {\n    color: $black !important;\n  }\n  .MuiOutlinedInput-root {\n    .MuiInputBase-input {\n      border: 1px solid;\n      border-color: $secondary-color;\n    }\n  }\n  .MuiInputBase-input:focus {\n    border: 1px solid $secondary-color;\n  }\n  .MuiOutlinedInput-notchedOutline {\n    border: 0;\n  }\n  .MuiOutlinedInput-multiline { \n    padding: 0 !important;\n  }\n  .MuiOutlinedInput-inputMultiline {\n    padding: 20px !important;\n  }\n  .MuiInputLabel-outlined {\n    background-color: $white;\n    color: $black;\n    padding: 0 5px;\n    z-index: 2;\n  }\n  .pm-button[data-cy='submit'] {\n    padding: 10px 20px;\n    background-color: $black !important;\n    font-size: 20px !important;\n    color: $white !important;\n    transition: 250ms;\n    text-transform: uppercase;\n    line-height: 1;\n    border-radius: 0;\n    letter-spacing: 2px;\n\n    font-family: $heading-font;\n    padding-bottom: 0.8em;\n  }\n}\n\n\n@mixin accent-font-style {\n  color: $secondary-color;\n  font-family: $accent-font;\n  font-size: 36px;\n  text-transform: unset;\n  letter-spacing: 0;\n}\n\n/* navigation */\n.navbar .nav > li > a, .navbar .nav > li > .a, .navbar .nav-dropdown .nav-dropdown-links a, .navbar .nav-dropdown .nav-dropdown-links .a {\n  letter-spacing: 1px;\n  font-size: 16px;\n  text-transform: uppercase;\n}\n.pm-link-featured {\n  background-color: $tertiary-color !important;\n  color: $black !important;\n}\n.no-custom-page-bg .navbar.navbar-bg-fxhbg_black, .navbar.toggle-scrolled.navbar-bg-fxhbg_black {\n  background-color: $black; \n}\n\n/* Remove if not using logo filter */\n.navbar.brand-fx-fxh_white.toggle-scrolled .navbar-brand {\n  filter: brightness(0) invert(1);\n}\n\n/* featured link */\n.navbar .pm-link-featured  {\n  position: relative;\n  z-index: 9; \n\n  &::before {\n    position: absolute;\n    top: -90%;\n    left: 0;\n    display: block;\n    content: '';\n    width: 100%;\n    height: 110px;\n    background: $tertiary-color;\n    clip-path: polygon(100% 0, 100% 75%, 50% 100%, 0 75%, 0 0);\n    z-index: -1;\n    transition: 500ms ease-in-out;\n\n  }\n\n  &:hover, &:focus {\n    opacity: 1 !important;\n    &::before {\n      background-color: darken($tertiary-color, 8%);\n      height: 120px;\n    }\n  }\n}\n\n/* navbar icons */\n.navbar-header .pop-icon {\n  color: $white;\n}\n\n/* page headers */\n.page-header .pm-page-heading, .page-header .pm-page-subheading {\n  text-shadow: 1px 1px 5px rgba($black, 0.4);\n}\n.page-header .header-bg.transition-fade.header-bg-show {\n  opacity: .8!important;\n  filter: contrast(1.2);\n}\n\n.page-header {\n  background-color: black;\n\n  .pm-page-subheading {\n    letter-spacing: 0;\n  }\n\n  .pm-page-heading {\n    font-size: 36px;\n    @media (min-width: 992px) {\n      font-size: 76px;\n    }\n  }\n}\n\n/* header image overlay */\n.pm-custom-page-wrap .container-fluid\n{\n  padding: 0;\n  &::after {\n    @include stamp-border-top;\n  }\n}\n\n/* blue border for pages where first section is not white */\n/*.pm-custom-home-wrap {\n.container-fluid::after {\nbackground-image: url(https://popmenucloud.com/izyapxve/f9c34329-02db-4576-8960-98e7b530b5f6.png);\n}\n}*/\n\n\n/*