 &::before {\n    position: absolute;\n    top: -90%;\n    left: 0;\n    display: block;\n    content: '';\n    width: 100%;\n    height: 110px;\n    background: $tertiary-color;\n    clip-path: polygon(100% 0, 100% 75%, 50% 100%, 0 75%, 0 0);\n    z-index: -1;\n    transition: 500ms ease-in-out;\n\n  }\n\n  &:hover, &:focus {\n    opacity: 1 !important;\n    &::before {\n      background-color: darken($tertiary-color, 8%);\n      height: 120px;\n    }\n  }\n}\n\n/* navbar icons */\n.navbar-header .pop-icon {\n  color: $white;\n}\n\n/* page headers */\n.page-header .pm-page-heading, .page-header .pm-page-subheading {\n  text-shadow: 1px 1px 5px rgba($black, 0.4);\n}\n.page-header .header-bg.transition-fade.header-bg-show {\n  opacity: .8!important;\n  filter: contrast(1.2);\n}\n\n.page-header {\n  background-color: black;\n\n  .pm-page-subheading {\n    letter-spacing: 0;\n  }\n\n  .pm-page-heading {\n    font-size: 36px;\n    @media (min-width: 992px) {\n      font-size: 76px;\n    }\n  }\n}\n\n/* header image overlay */\n.pm-custom-page-wrap .container-fluid\n{\n  padding: 0;\n  &::after {\n    @include stamp-border-top;\n  }\n}\n\n/* blue border for pages where first section is not white */\n/*.pm-custom-home-wrap {\n.container-fluid::after {\nbackground-image: url(https://popmenucloud.com/izyapxve/f9c34329-02db-4576-8960-98e7b530b5f6.png);\n}\n}*/\n\n\n/* buttons */\n.pm-button.pm-announcement-button, .menu-download a, .pm-custom-section-multi-column_section .pm-button, .pm-button.pm-about-section-button, .pagebutton, .page-button {\n  font-family: $heading-font;\n  background-color: lighten($black, 5%);\n  color: $white;\n  position: relative;\n  font-size: 18px;\n  outline: none;\n  border-radius: 0px;\n  margin: 30px 10px 0;\n  padding: 10px 33px 17px;\n  transition: all 500ms;\n  line-height: 1;\n  min-width: 280px;\n  text-align: center;\n  transition: ease 500ms;\n\n}\n.pm-button.pm-announcement-button:hover, .pm-button.pm-announcement-button:focus, .menu-