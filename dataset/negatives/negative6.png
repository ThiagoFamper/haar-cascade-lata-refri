function validateForm() {
  // Get form elements (assuming all forms use the same class names or IDs)
  const forms = document.querySelectorAll('.contact-form');
  
  forms.forEach((form) => {
    const emailInput = form.querySelector('.email');
    const firstNameInput = form.querySelector('.first-name');
    const lastNameInput = form.querySelector('.last-name');
    const submitButton = form.querySelector('.submit-button');
    
    // Check if email is in blocked list or if first name is the same as last name
    const isBlockedEmail = blockedEmails.includes(emailInput.value.trim().toLowerCase());
    const isSameName = firstNameInput.value.trim().toLowerCase() === lastNameInput.value.trim().toLowerCase();
    
    // Conditionally set pointer-events
    if (isBlockedEmail || isSameName) {
      submitButton.style.pointerEvents = 'none';
      submitButton.style.opacity = '0.5';  // Optional: Add a visual cue for disabled button
    } else {
      submitButton.style.pointerEvents = 'auto';
      submitButton.style.opacity = '1';  // Reset the visual cue
    }
  });
}

